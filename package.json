{
  "name": "bobtail-core",
  "version": "2.1.0",
  "repository": {
    "type": "git",
    "url": "git://github.com/bobtail-dev/bobtail-core.git"
  },
  "author": {
    "name": "Richard Mehlinger",
    "url": "http://rmehlinger.com/"
  },
  "main": "dist/main.js",
  "scripts": {
    "build": "npm run clean && npm run browserify && npm run minify",
    "clean": "rm -rf dist && mkdir dist",
    "browserify": "browserify src/main.js --verbose > dist/main.js",
    "minify": "uglifyjs -mc --source-map -o dist/main.min.js dist/main.js",
    "typescript": "",
    "test": "jasmine"
  },
  "devDependencies": {
    "babelify": "^7.3.0",
    "bower": "^1.8.0",
    "browserify": "^14.4.0",
    "coffeeify": "^2.1.0",
    "exorcist": "^0.4.0",
    "jasmine": "^2.6.0",
    "jasmine-core": "^2.5.2",
    "jquery": "^3.2.1",
    "typescript": "^2.1.6",
    "uglify-es": "^3.0.23"
  },
  "engines": {
    "node": ">=0.8.0"
  },
  "dependencies": {
    "underscore": "~1.8.3"
  }
}
